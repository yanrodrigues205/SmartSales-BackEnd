generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite" // MySQL ou SQLite dependendo da vari√°vel
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  createdAt DateTime @default(now())
}
